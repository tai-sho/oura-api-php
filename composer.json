{
    "name": "tai-sho/oura-api-php",
    "description": "A PHP client library for the Oura Ring API",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.0",
        "psr/http-message": "^1.0",
        "psr/http-client": "^1.0",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "OuraApiPhp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OuraApiPhp\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}

